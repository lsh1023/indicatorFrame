<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.indicatorframe">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FeaturedActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".utils.TestActivity" />
        <activity
            android:name=".FeaturedItemJumpActivity"
            android:taskAffinity="com.item" />
        <activity android:name=".find.FindSearchActivity" />
        <activity android:name=".find.finditem.hideleg.HideLegActivity" />
        <activity android:name=".find.finditem.reducefat.ReduceFatActivity" />
        <activity android:name=".find.finditem.stretch.StretchActivity" />
        <activity android:name=".find.finditem.shake.ShakeActivity"/>
    </application>

</manifest>